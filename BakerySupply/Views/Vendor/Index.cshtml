@{
    Layout = "_Layout";
}

@using BakerySupply.Models;

<h1> Here is a List of All of your Current Vendors</h1>

@if (@Model.Count == 0)
{
    <h3>You currently have no Vendors listed for your business!</h3>
}

@foreach (Vendor vendor in Model)
{
    <h3><a href="/vendor/@vendor.Id">@vendor.Name</a></h3>
}

<p><a href="/">Go Back To Home</a></p>
